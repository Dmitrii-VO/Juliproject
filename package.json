{
  "name": "education-materials-platform",
  "version": "1.0.0",
  "description": "Платформа для генерации учебных материалов с использованием ИИ",
  "main": "server.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "start": "node server.js",
    "install-all": "npm install && cd client && npm install"
  },
  "dependencies": {
    "archiver": "^6.0.2",
    "axios": "^1.5.0",
    "cors": "^2.8.5",
    "docx": "^8.5.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.10.0",
    "jspdf": "^2.5.2"
  },
  "devDependencies": {
    "concurrently": "^8.2.0",
    "nodemon": "^3.0.1"
  },
  "keywords": [
    "образование",
    "ИИ",
    "учебные материалы",
    "Россия"
  ],
  "author": "Educational Platform",
  "license": "MIT"
}
